@model WebApplication.Models.Lifecycle

<div>
    
</div>
<div class="modal-content">
    <div class="modal-header">
        <button class="close" data-dismiss="modal" area-hidden="true">X</button>
        <h4>Жизненный цикл</h4>
    </div>
    <div class="modal-body">
        <dl class="dl-horizontal">
            <p>
                <b>
                    Заявка № @Html.DisplayFor(m => m.Id)
                </b>
            </p>
            <p>
                <b>
                    Открыта:
                </b>
                    @Html.DisplayFor(m => m.Opened)
            </p>
            <p>
                <b>
                    Распределена:
                </b> 
                @Html.DisplayFor(m => m.Distributed)
            </p>
            <p>
                <b>
                    В процессе:
                </b>
                 @Html.DisplayFor(m => m.Proccesing)
            </p>
            <p>
                <b>
                    На проверке:
                </b> 
                @Html.DisplayFor(m => m.Checking)
            </p>
            <p>
                <b>Закрыта:</b>
                @Html.DisplayFor(m => m.Closed)
            </p>
        </dl>
    </div>
</div>